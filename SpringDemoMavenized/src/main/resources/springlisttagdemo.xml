<?xml version="1.0" encoding="UTF-8"?>
	<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">


			<bean id="triangleWithListOfPoints" class="com.spring.example.TriangleWithListOfPoints">
				<property name="points">
				<list>
					<ref bean="point1"/>
					<ref bean="point2"/>
					<ref bean="point3"/>
				</list>
				</property>
			</bean>
			
			
			<bean id ="point1" class="com.spring.example.Point">
				<property name="x" value ="0"></property>
				<property name="y" value ="0"></property>
			</bean>
			
			<bean id ="point2" class="com.spring.example.Point">
				<property name="x" value ="0"></property>
				<property name="y" value ="-20"></property>
			</bean>
			
			
			<bean id ="point3" class="com.spring.example.Point">
				<property name="x" value ="20"></property>
				<property name="y" value ="0"></property>
			</bean>






</beans>